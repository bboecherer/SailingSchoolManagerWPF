<mui:ModernWindow x:Class="SailingSchoolWPF.Pages.Invoicing.Update.UpdateInvoice"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:resx="clr-namespace:SailingSchoolWPF.Resources.Language"
        Title="{x:Static resx:Resource.Course}" IsTitleVisible="True"
        xmlns:Button="clr-namespace:SailingSchoolWPF.Pages.Controls"
       ContentSource="/Pages/Invoicing/Update/Base.xaml"
       Height="500" Width="500" Closing="ModernWindow_Closing"
      >
    <mui:ModernWindow.MenuLinkGroups >
        <mui:LinkGroup DisplayName="{x:Static resx:Resource.Update_ToolTip_Desc}">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="{x:Static resx:Resource.Base}" Source="/Pages/Invoicing/Update/Base.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
