<mui:ModernWindow x:Class="SealingSchoolWPF.Pages.Courses.Planing.PlaningCourse"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:resx="clr-namespace:SealingSchoolWPF.Resources.Language"
        Title="{x:Static resx:Resource.CoursePlanning}" IsTitleVisible="True"
       ContentSource="/Pages/Courses/Planing/PlaningPage1.xaml"
       Height="500" Width="500" Closed="ModernWindow_Closed_1"
      >
    <mui:ModernWindow.MenuLinkGroups >
        <mui:LinkGroup DisplayName="{x:Static resx:Resource.Update_ToolTip_Desc}">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="{x:Static resx:Resource.Base}" Source="/Pages/Courses/Planing/PlaningPage1.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.Instructor}" Source="/Pages/Courses/Planing/PlaningPage2.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
