<mui:ModernWindow x:Class="SealingSchoolWPF.Pages.Courses.Planing.CreateCoursePlaning"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:resx="clr-namespace:SealingSchoolWPF.Resources.Language"
        Title="KURSPLANUNG" IsTitleVisible="True"
       ContentSource="/Pages/Courses/Planing/Base.xaml"
       Height="500" Width="500" Closed="ModernWindow_Closed"
      >
    <mui:ModernWindow.MenuLinkGroups >
        <mui:LinkGroup DisplayName="{x:Static resx:Resource.Create_ToolTip_Desc}">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="{x:Static resx:Resource.Base}" Source="/Pages/Courses/Planing/Base.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.Instructor}" Source="/Pages/Courses/Planing/Instructor.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.Overview}" Source="/Pages/Courses/Planing/Overview.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
