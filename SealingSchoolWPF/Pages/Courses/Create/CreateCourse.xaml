<mui:ModernWindow x:Class="SealingSchoolWPF.Pages.Courses.Create.CreateCourse"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:resx="clr-namespace:SealingSchoolWPF.Resources.Language"
        Title="KURS" IsTitleVisible="True"
       ContentSource="/Pages/Courses/Create/Base.xaml"
       Height="450" Width="500" Closed="ModernWindow_Closed"
      >
    <mui:ModernWindow.MenuLinkGroups >
        <mui:LinkGroup DisplayName="{x:Static resx:Resource.Create_ToolTip_Desc}">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="{x:Static resx:Resource.Base}" Source="/Pages/Courses/Create/Base.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.Pricing}" Source="/Pages/Courses/Create/Financial.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.Notes}" Source="/Pages/Courses/Create/Notes.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.Documents}" Source="/Pages/Courses/Create/Documents.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>
</mui:ModernWindow>
