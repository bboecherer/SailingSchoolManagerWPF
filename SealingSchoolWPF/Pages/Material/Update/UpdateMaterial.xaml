<mui:ModernWindow x:Class="SailingSchoolWPF.Pages.Material.Update.UpdateMaterial"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:mui="http://firstfloorsoftware.com/ModernUI"
        xmlns:resx="clr-namespace:SailingSchoolWPF.Resources.Language"
        Title="Material" IsTitleVisible="True"
       ContentSource="/Pages/Material/Update/Base.xaml"
       Height="550" Width="500" Closed="ModernWindow_Closed"
      >

    <mui:ModernWindow.MenuLinkGroups>
        <mui:LinkGroup DisplayName="{x:Static resx:Resource.Update_ToolTip_Desc}">
            <mui:LinkGroup.Links>
                <mui:Link DisplayName="{x:Static resx:Resource.Base}" Source="/Pages/Material/Update/Base.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.MaterialGroup}" Source="/Pages/Material/Update/MaterialGroup.xaml" />
                <mui:Link DisplayName="{x:Static resx:Resource.Notes}" Source="/Pages/Material/Update/Notes.xaml" />
            </mui:LinkGroup.Links>
        </mui:LinkGroup>
    </mui:ModernWindow.MenuLinkGroups>

</mui:ModernWindow>
